{
  "expo": {
    "name": "spinach",
    "slug": "recipe_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tonelo.spinach",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserTrackingUsageDescription": "We use your data to provide a better experience."
      }
    },
    "android": {
      "package": "com.tonelo.spinach",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "spinach",
    "newArchEnabled": true,
    "extra": {
      "eas": {
        "projectId": "4b3086f9-eaf8-4f32-85b4-f8709d5a7442"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "owner": "tonelo"
  }
}
